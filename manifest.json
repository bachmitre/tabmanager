{
  "name": "Tab Manager",
  "description": "Organize your tabs on a desktop-like screen.",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
    "chrome_url_overrides": {
      "newtab": "index.html"
  }
}